{
  "QUnit: module": {
    "prefix": "module",
    "body": "module('${1:name}'${2:, ${3:hooks}});",
    "description": "QUnit: module"
  },
  "QUnit: test": {
    "prefix": "test",
    "body": "test('${1:name}', function(assert) {\n\t$0\n});",
    "description": "QUnit: test"
  },
  "QUnit: skip": {
    "prefix": "skip",
    "body": "skip('${1:name}', function(assert) {\n\t$0\n});",
    "description": "QUnit: skip"
  }
}
