{
  "QUnit: module": {
    "body": "module('${1:name}', function(${2:hooks}){\n  ${3}\n});",
    "description": "QUnit: module",
    "prefix": "module"
  },
  "QUnit: skip": {
    "body": "skip('${1:name}', function(assert) {\n  $0\n});",
    "description": "QUnit: skip",
    "prefix": "skip"
  },
  "QUnit: test": {
    "body": "test('${1:name}', function(assert) {\n  $0\n});",
    "description": "QUnit: test",
    "prefix": "test"
  },
  "QUnit: test (async)": {
    "body": "test('${1:name}', async function(assert) {\n  $0\n});",
    "description": "QUnit: test",
    "prefix": "atest"
  }
}
